{
	"name": "PL_INGEST_RAW_DATA",
	"properties": {
		"activities": [
			{
				"name": "CPY_RAW_DATA",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*.json",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['kind']"
								},
								"sink": {
									"name": "kind"
								}
							},
							{
								"source": {
									"path": "['etag']"
								},
								"sink": {
									"name": "etag"
								}
							},
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "['snippet']['channelId']"
								},
								"sink": {
									"name": "channelId"
								}
							},
							{
								"source": {
									"path": "['snippet']['title']"
								},
								"sink": {
									"name": "title"
								}
							},
							{
								"source": {
									"path": "['snippet']['assignable']"
								},
								"sink": {
									"name": "assignable"
								}
							}
						],
						"collectionReference": "$['items']",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "DS_JSON",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_PARQUET",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}